{
  "name": "Moralis_MCP",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/server.ts",
    "dev": "fastmcp dev src/server.ts",
    "lint": "prettier --check . && eslint . && tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write . && eslint --fix ."
  },
  "keywords": [
    "fastmcp",
    "mcp",
    "boilerplate"
  ],
  "author": "block",
  "type": "module",
  "license": "MIT",
  "description": "Moralis MCP Server",
  "dependencies": {
    "@moralisweb3/auth": "^2.27.2",
    "@moralisweb3/common-evm-utils": "^2.27.2",
    "@moralisweb3/common-sol-utils": "^2.27.2",
    "@moralisweb3/core": "^2.7.4",
    "@moralisweb3/evm-api": "^2.27.2",
    "@moralisweb3/sol-api": "^2.27.2",
    "dotenv": "^16.5.0",
    "fastmcp": "^1.27.3",
    "moralis": "^2.27.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node22": "^22.0.1",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-perfectionist": "^4.12.3",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  }
}
